{
  "name": "sz-rat",
  "version": "0.7",
  "sz_rat": {
    "version": "0.7",
    "vname": "U235",
    "description": "Seymour Zhang's React Application Template",
    "repo": "https://github.com/zsh2401/sz-rat"
  },
  "description": "An application powered by sz-rat",
  "main": "./scripts/server",
  "scripts": {
    "deploy": "node ./scripts/deploy.js",
    "build": "cross-env TS_NODE_PROJECT=\"./scripts/tsconfig-for-webpack-config.json\" webpack --config ./scripts/webpack.config.prod.ts",
    "build:dev": "cross-env TS_NODE_PROJECT=\"./scripts/tsconfig-for-webpack-config.json\" webpack --config ./scripts/webpack.config.dev.ts",
    "server": "ts-node ./scripts/server/index.ts",
    "start": "cross-env TS_NODE_PROJECT=\"./scripts/tsconfig-for-webpack-config.json\" webpack-dev-server --config ./scripts/webpack.config.dev.ts",
    "start:prod": "cross-env TS_NODE_PROJECT=\"./scripts/tsconfig-for-webpack-config.json\" webpack-dev-server --config ./scripts/webpack.config.prod.ts"
  },
  "keywords": [
    "react",
    "react-router",
    "SPA",
    "PWA"
  ],
  "author": "zsh2401",
  "license": "MIT",
  "devDependencies": {
